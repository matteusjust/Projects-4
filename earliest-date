#include <stdio.h>

int main(void)
{
    int day1, day2, month1, month2, year1, year2;

    printf ("Enter a date (mt/day/year): ");
    scanf ("%d/%d/%d", &month1, &day1, &year1);

    printf ("\nEnter a second date(mt/day/year): ");
    scanf ("%d/%d/%d", &month2, &day2, &year2);

    if (year1 > year2){
        printf ("%d/%d/%d is the earliest date", month2, day2, year2);
    } else if (year1 == year2) {
        } if (month1 > month2) {
            printf ("%d/%d/%d is the earliest date", month2, day2, year2);
        } else if (month1 == month2){
          }  if (day1 > day2){
                printf ("%d/%d/%d is the earliest date", month2, day2, year2);
        } else {
            printf ("%d/%d/%d is the earliest date", month1, day1, year1);
        }

        return 0;
    
}
